import BaseLayout from '../../../../layouts/BaseLayout.astro';
import DocsLayout from '../../../../layouts/DocsLayout.astro';
import { markdown } from '@astropub/md'
export async function getStaticPaths() {

  return pages.map(({ bibtex }) => {
    return {
      params: { bibtex },
      props: { bibtex },
    };
